<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- This module doesn't create new payment acquirer, it extends existing Mollie one -->
        <record id="ir_cron_mollie_subscription_renewal" model="ir.cron">
            <field name="name">Mollie: Process Subscription Renewals</field>
            <field name="model_id" ref="model_sale_subscription"/>
            <field name="state">code</field>
            <field name="code">model.cron_process_subscription_renewals()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
        </record>
    </data>
</odoo>